class A1Q2 {
    public static void main(String[] args) {
     int h=0;
     int m=0;
     int s=0;
     System.out.print("Enter initial time h: m: s: ");
    int hh=0;
    int mm=0;
    int ss=0;
    System.out.print("Enter final time hh: mm: ss:");
float f;
    float ff;
    float i;

    {
        ff = (3600 * hh + 60 * mm + ss);
        f = (3600 * h + 60 * m + s);
        i = ff - f;
    }
    System.out.print(i/3600,i%3600%60,i%3600/60);

}}